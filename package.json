{
  "name": "@pubinfo/changelog",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.15.4",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "changelog": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.12 || >=20.9"
  },
  "scripts": {
    "commit": "pnpm git cz",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm eslint .",
    "lint:fix": "pnpm eslint . --fix",
    "release": "pnpm bumpp",
    "dev-view": "npx taze -r ",
    "dev-update": "npx taze major -wr",
    "prepare": "pnpm husky install",
    "publish": "npm publish --registry https://registry.npmjs.org/"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "boxen": "^7.1.1",
    "c12": "^1.10.0",
    "chalk": "^5.3.0",
    "colorette": "^2.0.20",
    "commitlint": "^19.0.3",
    "consola": "^3.2.3",
    "convert-gitmoji": "^0.1.5",
    "execa": "^8.0.1",
    "mri": "^1.2.0",
    "node-fetch-native": "^1.6.2",
    "ofetch": "^1.3.3",
    "open": "^10.1.0",
    "pathe": "^1.1.2",
    "pkg-types": "^1.0.3",
    "scule": "^1.3.0",
    "semver": "^7.6.0",
    "std-env": "^3.7.0",
    "typescript": "^5.3.3",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "bumpp": "^9.4.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "unbuild": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
