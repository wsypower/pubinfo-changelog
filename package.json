{
  "name": "@pubinfo/changelog",
  "type": "module",
  "version": "1.0.4",
  "packageManager": "pnpm@8.15.4",
  "description": "Generate Beautiful Changelogs using Conventional Commits",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsypower/pubinfo-changelog"
  },
  "bugs": {
    "url": "https://github.com/wsypower/pubinfo-changelog/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "changelog": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.12 || >=20.9"
  },
  "scripts": {
    "commit": "pnpm git cz",
    "build": "pnpm unbuild",
    "dev": "pnpm unbuild --stub",
    "changelogen": "jiti ./src/cli.ts",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "lint:fix": "eslint --ext .ts,.js,.mjs,.cjs . --fix",
    "dev-view": "npx taze -r ",
    "dev-update": "npx taze major -wr",
    "prepare": "pnpm husky install",
    "release": "pnpm bumpp"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "boxen": "^7.1.1",
    "c12": "^1.10.0",
    "chalk": "^5.3.0",
    "colorette": "^2.0.20",
    "commitlint": "^19.3.0",
    "consola": "^3.2.3",
    "convert-gitmoji": "^0.1.5",
    "execa": "^9.0.0",
    "mri": "^1.2.0",
    "node-fetch-native": "^1.6.4",
    "ofetch": "^1.3.4",
    "open": "^10.1.0",
    "pathe": "^1.1.2",
    "pkg-types": "^1.1.0",
    "scule": "^1.3.0",
    "semver": "^7.6.1",
    "std-env": "^3.7.0",
    "typescript": "5.4.5",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "bumpp": "^9.4.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.2",
    "unbuild": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
